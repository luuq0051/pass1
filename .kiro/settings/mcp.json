{
  "mcpServers": {
    "playwright": {
      "command": "uvx",
      "args": [
        "mcp-playwright@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "playwright_navigate",
        "playwright_click",
        "playwright_fill",
        "playwright_get_visible_text",
        "playwright_get_visible_html",
        "playwright_close",
        "get_page_title",
        "take_screenshot",
        "create_browser_session"
      ]
    },
    "supabase": {
      "command": "curl",
      "args": [
        "-s",
        "https://mcp.supabase.com/mcp?read_only=false&project_ref=tzvxhmrnaezqrchfgems"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": []
    }
  }
}